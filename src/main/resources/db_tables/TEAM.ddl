DROP TABLE TEAM;

CREATE TABLE TEAM (
	TEAM_ID 	INT NOT NULL AUTO_INCREMENT PRIMARY KEY,
	CITY_ID 	INT,
	TEAM_NAME	VARCHAR(20),
    MASCOT		VARCHAR(20),
    WINS		INT,
    LOSSES		INT,
    TIES		INT,

    CONSTRAINT FK_CITY
    FOREIGN KEY (CITY_ID)
    REFERENCES CITY(CITY_ID)
);

ALTER TABLE TEAM
DROP CONSTRAINT FK_CITY;

ALTER TABLE TEAM
ADD CONSTRAINT FK_CITY
FOREIGN KEY (CITY_ID)
REFERENCES CITY(CITY_ID);